{
  "name": "xtream-codes-proxy",
  "version": "1.0.0",
  "description": "将M3U服务器转换为Xtream Codes格式的代理服务器，支持用户认证和并发限制",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-pm2": "npm install -g pm2",
    "start-pm2": "pm2 start index.js --name xtream-proxy",
    "stop-pm2": "pm2 stop xtream-proxy",
    "restart-pm2": "pm2 restart xtream-proxy",
    "logs-pm2": "pm2 logs xtream-proxy"
  },
  "keywords": [
    "xtream-codes",
    "iptv",
    "m3u",
    "proxy",
    "streaming",
    "live-tv",
    "media-server"
  ],
  "author": "Xtream Codes Proxy Developer",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "node-telegram-bot-api": "^0.64.0",
    "crypto": "^1.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/xtream-codes-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/xtream-codes-proxy/issues"
